{
  "configurations": [
    {
      "name": "linux-gcc-x64",
      "intelliSenseMode": "linux-gcc-x64",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/src"
      ],
      "defines": [
        "_DEBUG"
      ],
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "forcedInclude": [
        "${workspaceFolder}/src/PCH.hpp"
      ],
      "compilerArgs": [
        "-Wall",
        "-Wextra",
        "-Wpedantic"
      ],
      "compilerPath": "/usr/bin/gcc"
    },
    {
      "name": "Mac",
      "intelliSenseMode": "clang-x64",
      "compilerPath": "/usr/bin/clang",
      "macFrameworkPath": [
        "/Library/Frameworks",
        "/System/Library/Frameworks"
      ],
      "includePath": [
        "${workspaceFolder}/src",
        "${workspaceFolder}/lib",
        "${workspaceFolder}/test",
        "/usr/local/include/**"
      ],
      "defines": [
        "_DEBUG"
      ],
      "cStandard": "c11",
      "cppStandard": "c++17",
      "forcedInclude": [
        "${workspaceFolder}/src/PCH.hpp"
      ]
    },
    {
      "name": "Win32",
      "intelliSenseMode": "gcc-x86",
      "compilerPath": "/usr/bin/x86_64-w64-mingw32-g++-win32",
      "includePath": [
        "${workspaceFolder}/src",
        "${workspaceFolder}/lib",
        "${workspaceFolder}/test",
        "C:/SFML-2.5.1/include",
        "/usr/include/x86_64-linux-gnu/c++/10"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE"
      ],
      "cStandard": "c11",
      "cppStandard": "c++17",
      "forcedInclude": [
        "${workspaceFolder}/src/PCH.hpp"
      ]
    }
  ],
  "version": 4
}